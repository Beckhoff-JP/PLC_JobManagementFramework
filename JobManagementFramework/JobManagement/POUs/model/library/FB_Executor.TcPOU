    <Method Name="execute" Id="{12a00ca6-fc73-4fe5-8b5c-2a7a9dcde6d1}">
      <Declaration><![CDATA[{warning 'add method implementation '}
METHOD execute : BOOL
VAR_INST
	_state_mem :E_FutureExecutionState;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _activity = 0 THEN
	execute := TRUE;
	RETURN;
ELSE
	execute := FALSE;
END_IF

CASE _state OF
	E_FutureExecutionState.idle: 
		init();
		_state := E_FutureExecutionState.init;
	E_FutureExecutionState.init:
		IF _activity.init() THEN
			IF SUCCEEDED(_activity.result) THEN
				_ready := TRUE;
				_state := E_FutureExecutionState.wait_for_process;
			ELSE
				_result := _activity.result;
				_state := E_FutureExecutionState.abort;
				_aborting := TRUE;
			END_IF
		END_IF
	E_FutureExecutionState.wait_for_process: 
		IF _start THEN
			_state := E_FutureExecutionState.process;
			_start := FALSE;
		END_IF
	E_FutureExecutionState.process: 
		IF _activity.execute() THEN
			IF SUCCEEDED(_activity.result) THEN
				_state := E_FutureExecutionState.quit;
				_done := TRUE;
			ELSE
				_result := _activity.result;
				_state := E_FutureExecutionState.abort;
				_aborting := TRUE;
			END_IF
		END_IF
	E_FutureExecutionState.abort:
		IF _aborting AND _activity.abort() THEN
			_aborting := FALSE;
		END_IF
		IF _start AND NOT _aborting THEN
			_result := 0;
			_state := _last_state;
			_start := FALSE;
		END_IF
	E_FutureExecutionState.quit:
		IF _activity.quit() THEN
			IF SUCCEEDED(_activity.result) THEN
				_state := E_FutureExecutionState.finish;
			ELSE
				_result := _activity.result;
				_state := E_FutureExecutionState.abort;
				_aborting := TRUE;
			END_IF
		END_IF
	E_FutureExecutionState.finish:
		_result := 0;
		execute := TRUE;
END_CASE

IF _abort AND (
				_state = E_FutureExecutionState.init OR
				_state = E_FutureExecutionState.process OR
				_state = E_FutureExecutionState.quit
		)THEN
	_state := E_FutureExecutionState.abort;
	_aborting := TRUE;
	_result := -1;
END_IF

_abort := FALSE;

IF _state <> _state_mem THEN
	_last_state := _state_mem;
END_IF

_state_mem := _state;]]></ST>
      </Implementation>
    </Method>
