	_ready : BOOL;
	_start : BOOL;
	_abort : BOOL;
	_state : E_FutureExecutionState := E_FutureExecutionState.idle;
	_last_state  :E_FutureExecutionState;
	_activity : InterfaceFuture;
	_result : HRESULT;
	_aborting :BOOL;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
