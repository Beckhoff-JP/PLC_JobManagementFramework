	_done : BOOL;
	_start : BOOL;
	_resume : BOOL;
	_passive_abort : BOOL;
	_quit : BOOL;
	_upstream_aborting : BOOL := FALSE;
	 {attribute 'TcAnalytics'}
	 _state :E_FutureExecutionState := E_FutureExecutionState.idle;
