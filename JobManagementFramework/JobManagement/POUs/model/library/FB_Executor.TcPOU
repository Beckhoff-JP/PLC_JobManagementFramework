    <Method Name="get_job_info" Id="{776089a7-403b-4f19-b673-2cf0c4b27716}">
      <Declaration><![CDATA[METHOD get_job_info : BOOL
VAR_INPUT
	title		: T_MaxString; // Observable object title
	observable 	: I_Observable; // Observable object
	recursive	: BOOL := FALSE; // Set observable to child job recursively or not
END_VAR
VAR
	_info : I_JobInfo;
	_child_job : I_Job;
	_cmp : BOOL;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF observable = 0  OR NOT __QUERYINTERFACE(observable, _info) THEN
	get_job_info := TRUE;
	RETURN;
END_IF

_info.title := title;
_info.job := THIS^;
_info.last_state := _last_state;
_info.new_state := _state;
IF _current_timestamp <> 0 THEN
	_info.record_time := _current_timestamp^;
ELSE
	_info.record_time := F_GetSystemTime();
END_IF

observable.notify_observers();

IF recursive THEN
	_cmp := TRUE;
	IF THIS^.children <> 0 THEN
		REPEAT
			_child_job := THIS^.children.each_executor;
			IF _child_job <> 0 THEN
				IF NOT _child_job.get_job_info(title, observable, recursive) THEN
					_cmp := FALSE;
				END_IF
			END_IF
		UNTIL
			_child_job = 0
		END_REPEAT
	END_IF
	
	get_job_info := _cmp;
ELSE
	get_job_info := TRUE;
END_IF]]></ST>
      </Implementation>
    </Method>
    <Property Name="id" Id="{cfcf1743-1715-4af2-a0a4-cd9f1ebd1808}">
      <Declaration><![CDATA[PROPERTY id : T_MaxString // Get job id.
]]></Declaration>
      <Get Name="Get" Id="{5863c94c-93ce-4810-8c6f-f18910d5e32a}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[id := _id;]]></ST>
        </Implementation>
      </Get>
      <Set Name="Set" Id="{9e8cd938-613c-4a5e-bd20-a66e0b88a473}">
        <Declaration><![CDATA[VAR
	_eval_executor : I_Job;
	_parent_path : T_MaxString;
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[_id := id;
_eval_executor := _parent_job;

_parent_path := '';
REPEAT
	IF _eval_executor <> 0 THEN
		_parent_path := CONCAT(_eval_executor.id, _parent_path);
		_parent_path := CONCAT('/',_parent_path);
		_eval_executor := _eval_executor.parent_executor;
	END_IF
UNTIL
	_eval_executor = 0
END_REPEAT

_uid := CONCAT(_parent_path, '/');
_uid := CONCAT(_uid, _id);

IF _parent_job <> 0 AND_THEN NOT _parent_job.persistent_job THEN
	_namespace := _parent_job.namespace;
ELSE
	_namespace := _parent_path;
END_IF

_lid := RIGHT(_uid,LEN(_uid) - LEN(_namespace));]]></ST>
        </Implementation>
      </Set>
    </Property>
