]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _asset = 0 THEN
	_error_id := -1;
	RETURN;
END_IF

IF _asset.set_parameters() THEN
	init := TRUE;
END_IF
]]></ST>
