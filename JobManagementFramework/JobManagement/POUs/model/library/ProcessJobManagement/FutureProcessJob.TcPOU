    <Method Name="execute" Id="{417962f7-daf8-4dae-990a-9d97785b54f5}">
      <Declaration><![CDATA[METHOD execute : BOOL // Return TRUE if finish.
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _asset = 0 THEN
	_error_id := -1;
	RETURN;
END_IF

_asset.select_process_job(_process_job_name);
_asset.set_parameters();
_asset.process_job_start();
]]></ST>
