    <Method Name="init" Id="{8b026f10-89d6-40c5-a4e1-421de953942d}">
      <Declaration><![CDATA[METHOD init : BOOL // Return TRUE if finish.
VAR
	_each_executor : i_Job;
	_init_done : BOOL;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[_init_done := TRUE;
REPEAT
	_each_executor := THIS^.each_executor;
	IF _each_executor <> 0 THEN
		IF NOT _each_executor.init() THEN
			_init_done := FALSE;
		END_IF
	END_IF
UNTIL
	_each_executor = 0
END_REPEAT

_error_id := 0;
init := _init_done;]]></ST>
      </Implementation>
