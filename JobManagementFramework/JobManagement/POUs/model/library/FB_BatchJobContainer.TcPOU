        </Implementation>
      </Set>
    </Property>
    <Method Name="init" Id="{5ef2a4ac-f57b-470f-97a3-ba7305857e0b}">
      <Declaration><![CDATA[METHOD init : BOOL // Return TRUE if finish.
VAR_INST
	i :UINT;
	//possible_to_start :BOOL;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[FOR i := 1 TO _total_futures DO
	_executors[i]^.job_event_reporter := _parent_executor.job_event_reporter;
	_executors[i]^.init();
END_FOR

current_executing_future_id := 1;
_error_id := 0;
init := TRUE;]]></ST>
      </Implementation>
    </Method>
