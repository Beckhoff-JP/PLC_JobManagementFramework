    <Method Name="create_future" Id="{f7e25f65-449b-4ef3-a719-946b3d87fba9}">
      <Declaration><![CDATA[METHOD create_future : I_Future
VAR_INPUT
	future_name	: STRING;
END_VAR
VAR
	_p_future : POINTER TO FB_ParallelJobContainer;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[_p_future := __NEW(FB_ParallelJobContainer);
_p_future^.future_name := future_name;
create_future := _p_future^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="destroy_future" Id="{059ba90e-ca78-49a7-9d25-013656fe00e0}">
      <Declaration><![CDATA[METHOD destroy_future : I_Future
VAR_INPUT
	future	: I_Future;
END_VAR
VAR
	_p_future : POINTER TO FB_ParallelJobContainer;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF __QUERYPOINTER(future, _p_future) THEN
	__DELETE(_p_future);
	IF _p_future = 0  THEN
		destroy_future := 0;
	END_IF
END_IF
]]></ST>
      </Implementation>
    </Method>
  </POU>
