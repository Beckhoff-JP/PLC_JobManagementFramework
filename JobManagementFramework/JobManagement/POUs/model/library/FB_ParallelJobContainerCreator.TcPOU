  <POU Name="FB_ParallelJobContainerCreator" Id="{808db55b-c727-45ae-8e1f-ebf797e858e3}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_ParallelJobContainerCreator IMPLEMENTS I_TaskCreator
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
